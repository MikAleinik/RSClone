{
  "name": "server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && node dist/server.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/server.js",
    "start:dev": "nodemon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^3.0.2",
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^7.4.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/jwt": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^7.4.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.1.0",
    "@fastify/type-provider-typebox": "^2.3.0",
    "@sinclair/typebox": "^0.24.27",
    "dotenv": "^16.0.1",
    "fastify": "^4.0.0",
    "fastify-auth": "^1.2.0",
    "fastify-cli": "^5.0.1",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg-promise": "^10.11.1"
  },
  "devDependencies": {
    "@fastify/jwt": "^5.0.0",
    "@types/fastify-jwt": "^0.8.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.7.1",
    "@types/tap": "^15.0.5",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^3.2.0",
    "fastify-tsconfig": "^1.0.1",
    "json-schema-to-ts": "^2.5.5",
    "nodemon": "^2.0.19",
    "pino": "^8.4.1",
    "pino-pretty": "^9.0.0",
    "prettier": "2.2.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4"
  }
}
